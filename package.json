{
  "name": "ai-chatbot-plugin",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/chatbot.umd.js",
  "module": "./dist/chatbot.js",
  "style": "./dist/ai-chatbot-plugin.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsc && vite build --mode lib",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chatbot.js",
      "require": "./dist/chatbot.umd.js"
    },
    "./style": "./dist/ai-chatbot-plugin.css",
    "./css": "./dist/ai-chatbot-plugin.css"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
